<script>
    function handleLogout() {
      fetch('/logout', {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json',
        },
      })
        .then((response) => {
          if (response.ok) {
            // Redirect to login page
            window.location.href = '/login';
          } else {
            console.error('Logout failed.');
          }
        })
        .catch((error) => {
          console.error('Error:', error);
        });
    }
  </script>
  
